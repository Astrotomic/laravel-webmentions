{
    "name": "astrotomic/laravel-webmentions",
    "description": "",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "astrotomic",
        "laravel-webmentions",
        "laravel",
        "webmentions",
        "webmention"
    ],
    "authors": [
        {
            "name": "Tom Witkowski",
            "email": "gummibeer@astrotomic.info",
            "homepage": "https://gummibeer.dev",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/astrotomic/laravel-webmentions",
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.0.1",
        "illuminate/collections": "^9.0 || ^10.0 || ^11.0",
        "illuminate/http": "^9.0 || ^10.0 || ^11.0",
        "illuminate/support": "^9.0 || ^10.0 || ^11.0"
    },
    "require-dev": {
        "gajus/dindent": "^2.0",
        "orchestra/testbench": "^7.0 || ^8.0 || ^9.0",
        "phpunit/phpunit": "^9.3 || ^10.0"
    },
    "autoload": {
        "psr-4": {
            "Astrotomic\\Webmentions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Astrotomic\\Webmentions\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Astrotomic\\Webmentions\\WebmentionsServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
